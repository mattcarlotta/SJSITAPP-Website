{
  "name": "sjsitapp",
  "version": "2.2.1",
  "description": "An application for the Sharks Ice Team.",
  "repository": "https://github.com/mattcarlotta/SJITApp-Website",
  "sideEffects": false,
  "author": "Matt Carlotta",
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "analyze": "rimraf .next/analyze && analyze=true npm run build",
    "build": "npm run build:api && concurrently --success first --kill-others \"npm run start:api\" \"npm run build:app\"",
    "build:api": "rimraf build && tsc --p ./api/tsconfig.prod.json",
    "build:app": "LOAD_CONFIG=prod NODE_OPTIONS='-r @noshot/env' next build",
    "build:stage": "LOAD_CONFIG=stage NODE_OPTIONS='-r @noshot/env' next build",
    "build:staging": "npm run build:api && npm run build:stage",
    "dev": "concurrently  \"npm run dev:api\" \"npm run dev:app\"",
    "dev:app": "LOAD_CONFIG=dev NODE_OPTIONS='--max_old_space_size=4096 --inspect -r @noshot/env' next dev",
    "dev:api": "nodemon --config ./api/nodemon.json",
    "drop:dev": "LOAD_CONFIG=teardowndev ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/teardownDB/index.ts",
    "drop:prod": "LOAD_CONFIG=teardownprod ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/teardownDB/index.ts",
    "drop:stage": "LOAD_CONFIG=teardownstage ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/teardownDB/index.ts",
    "lint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./src --ext .ts,.tsx",
    "lint:api": "eslint -c ./api/.eslintrc --ignore-path ./api/.eslintignore ./api --ext .ts,.tsx",
    "seed:dev": "LOAD_CONFIG=seeddev ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/seedDB/index.ts",
    "seed:prod": "LOAD_CONFIG=seedprod ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/seedDB/index.ts",
    "seed:stage": "LOAD_CONFIG=seedstage ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./api/database/seedDB/index.ts",
    "stage": "npm run build:staging && npm run start:staging",
    "start": "npm run start:api & npm run start:app",
    "start:api": "LOAD_CONFIG=prod node -r ./api/prod-paths.js -r @noshot/env build/api/server.js",
    "start:apistage": "LOAD_CONFIG=stage node -r ./api/prod-paths.js -r @noshot/env build/api/server.js",
    "start:app": "LOAD_CONFIG=prod NODE_OPTIONS='-r @noshot/env' next start",
    "start:appstaging": "LOAD_CONFIG=stage NODE_OPTIONS='-r @noshot/env' next start",
    "start:staging": "npm run start:apistage & npm run start:appstaging",
    "test": "jest --config ./jest.json --passWithNoTests",
    "test:api": "LOAD_CONFIG=test jest --config ./api/jest.json --passWithNoTests",
    "test:apicov": "LOAD_CONFIG=test jest --config ./api/jest.json --coverage",
    "test:apiwatch": "LOAD_CONFIG=test jest --config ./api/jest.json --watchAll",
    "test:cov": "jest --config ./jest.json --coverage",
    "test:e2e": "cypress open --project ./e2e",
    "test:e2erun": "npm run build:staging && concurrently --success first --kill-others \"npm run start:staging\" \"cypress run --config video=false --project ./e2e\"",
    "test:failed": "jest --config ./jest.json --watch --onlyFailures",
    "test:prod": "npm run build:prod && npm run start",
    "test:stage": "LOAD_CONFIG=stage NODE_OPTIONS='-r @noshot/env' next build && npm run staging",
    "test:watch": "jest --config ./jest.json --watch --onlyChanged",
    "test:watchall": "jest --config ./jest.json --watchAll",
    "type:app": "tsc --p ./tsconfig.json",
    "type:api": "tsc --p ./api/tsconfig.json"
  },
  "dependencies": {
    "@date-io/moment": "1.x",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/data-grid": "^4.0.0-alpha.30",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "^3.3.10",
    "@noshot/env": "^1.0.1",
    "@react-icons/all-files": "^4.1.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.1",
    "compression": "^1.7.4",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fuse.js": "^6.4.6",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.random": "^3.2.0",
    "lodash.sortby": "^4.7.0",
    "lodash.throttle": "^4.1.1",
    "moment-timezone": "^0.5.33",
    "mongoose": "5.11.15",
    "mongoose-paginate-v2": "^1.3.18",
    "morgan": "^1.10.0",
    "next": "^10.2.1",
    "next-redux-wrapper": "^6.0.2",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-resize-detector": "^6.7.2",
    "react-toastify": "^7.0.4",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "ts-jest": "^27.0.2",
    "victory": "^35.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@emotion/jest": "^11.3.0",
    "@noshot/utils": "^1.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.35",
    "@types/compression": "^1.7.0",
    "@types/cookie-session": "^2.0.42",
    "@types/cors": "^2.8.10",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/jsdom": "^16.2.10",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.isempty": "^4.4.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.random": "^3.2.6",
    "@types/lodash.sortby": "^4.7.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/mongoose": "^5.11.93",
    "@types/mongoose-paginate-v2": "^1.3.10",
    "@types/morgan": "^1.9.2",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^17.0.6",
    "@types/react-redux": "^7.1.16",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "browserslist": "^4.16.6",
    "concurrently": "^6.2.0",
    "cypress": "^7.4.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-watch": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "26.6.3",
    "jest-css-modules-transform": "^4.2.1",
    "jest-enzyme": "^7.1.2",
    "jsdom": "16.5.3",
    "nodemon": "^2.0.7",
    "opener": "^1.5.2",
    "prettier": "^2.3.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga-test-plan": "^4.0.1",
    "supertest": "^6.1.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2",
    "webpack": "^5.37.1",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
